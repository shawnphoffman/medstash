{
	"name": "medstash",
	"version": "0.0.14",
	"description": "Self-hostable medical receipt storage system for HSA recordkeeping",
	"private": true,
	"workspaces": [
		"frontend",
		"backend"
	],
	"engines": {
		"node": ">=20.0.0",
		"npm": ">=9.0.0"
	},
	"scripts": {
		"dev": "docker compose -f docker-compose.dev.yml up",
		"build": "docker compose build",
		"start": "docker compose up",
		"test": "npm run test -w backend && npm run test -w frontend",
		"test:backend": "npm run test -w backend",
		"test:frontend": "npm run test -w frontend",
		"test:watch": "npm run test:watch -w backend",
		"test:watch:backend": "npm run test:watch -w backend",
		"test:watch:frontend": "npm run test:watch -w frontend",
		"test:coverage": "npm run test:coverage -w backend && npm run test:coverage -w frontend",
		"test:coverage:backend": "npm run test:coverage -w backend",
		"test:coverage:frontend": "npm run test:coverage -w frontend",
		"clean": "rm -rf frontend/dist backend/dist frontend/node_modules backend/node_modules node_modules",
		"version:patch": "node scripts/version.js patch",
		"version:minor": "node scripts/version.js minor",
		"version:major": "node scripts/version.js major",
		"version": "npm run version:patch"
	},
	"keywords": [
		"hsa",
		"receipts",
		"medical",
		"FIRE",
		"healthcare",
		"financial independence",
		"self-hosted"
	],
	"author": "Shawn Hoffman",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/shawnphoffman/medstash.git"
	},
	"bugs": {
		"url": "https://github.com/shawnphoffman/medstash/issues"
	},
	"homepage": "https://github.com/shawnphoffman/medstash#readme"
}
